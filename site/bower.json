{
  "name": "polymer-starter-kit",
  "private": true,
  "dependencies": {
    "bootstrap-sass": "~3.3.5",
    "modernizr": "~2.8.1",
    "leaflet": "~0.7.7",
    "d3": "~3.5.12",
    "bootstrap-material-design": "~0.5.7",
    "vue": "~1.0.13",
    "lodash": "~3.10.1",
    "nouislider": "~8.2.1",
    "wnumb": "~1.0.2"
  },
  "overrides": {
    "bootstrap-sass": {
      "main": [
        "assets/stylesheets/_bootstrap.scss",
        "assets/fonts/bootstrap/*",
        "assets/javascripts/bootstrap.js"
      ]
    },
    "bootstrap-material-design": {
      "main": [
        "sass/bootstrap-material-design.scss",
        "sass/ripples.scss",
        "sass/plugins/_plugin-nouislider.scss",
        "dist/js/material.js",
        "dist/js/ripples.js"
      ]
    }
  },
  "devDependencies": {
    "chai": "~3.4.2",
    "mocha": "~2.3.4"
  }
}
